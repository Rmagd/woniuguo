.container {
    min-height: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
}

.containers {
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
}

navigator:hover {
    background: none;
}

.color-read {
    color: red;
}

.iconfont {
    font-size: 30rpx;
}

.confirm-btn {
    background: #63B1EA;
    width: 95%;
    margin: 20rpx 2.5%;
    height: 80rpx;
    line-height: 80rpx;
    color: #FFF;
    font-size: 30rpx;
}

.record-empty {
    width: 100%;
    text-align: center;
    font-size: 30rpx;
    position: relative;
}

.record-empty-line {
    background: #E7E7E7;
    height: 1rpx;
    width: 70%;
    margin: 0 auto;
    position: absolute;
    top: 300rpx;
    left: 15%;
}

.record-empty text {
    position: relative;
    z-index: 99;
    background: #FFF;
    color: #666;
    top: 280rpx;
    padding: 10rpx 20rpx;
    border-radius: 10rpx;
}

.order-empty {
    text-align: center;
    color: #14B5FA;
    line-height: 500rpx;
    font-size: 50rpx;
}

.order-item {
    background: #FFF;
    border-bottom: 1rpx solid #EEEEEE;
    font-size: 30rpx;
    padding: 10rpx 20rpx;
}

.order-item-block {
    padding: 10rpx 0;
}

.order-item-ordernumber {
    color: #f90;
}

.order-item-constomername {
    float: right;
    color: #14B5FA;
}

.order-item-startTime,.order-item-endTime {
    color: #666666;
}

.order-item-endTime {
    margin-left: 30rpx;
}

