.btn-group {
    position: fixed;
    bottom: 0;
    box-shadow: 0 0 3px #ccc;
    display: flex;
    width: 100%;
    background: #FFF;
}

.btn-group .active {
    color: #20AD20;
}

.btn-item {
    flex: 1;
    text-align: center;
    padding: 10rpx 0;
}

.btn-item text {
    display: block;
    font-size: 22rpx;
}

.btn-item .iconfont {
    font-size: 40rpx;
}

.btn-group-icon {
    width: 45rpx;
    height: 45rpx;
}

.auth-view {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    z-index: 9999;
}

.authorize {
    background: none;
    box-shadow: none;
    margin: 120rpx auto 0;
    padding-top: 20rpx;
    width: 640rpx;
    height: 840rpx;
