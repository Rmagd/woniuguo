page {
    background: #f2f2f2;
}

.switch-panel {
    width: 100%;
    height: 100%;
    position: relative;
    font-size: 30rpx;
}

.switch-panel-title {
    background: #fff;
    color: #333;
    display: flex;
    height: 100rpx;
    line-height: 100rpx;
}

.switch-panel-title .active {
    border-bottom: 1rpx solid #17ab1b;
    color: #17ab1b;
}

.switch-panel-title-item {
    flex: 1;
    text-align: center;
}

.switch-panel-swiper {
    width: 100%;
    height: 1000rpx;
}

.switch-panel-content {
    width: 100%;
    height: 100%;
    padding-bottom: 100rpx;
}

.order-module {
    background: #fff;
    border-radius: 10rpx;
    width: 90%;
    margin: 20rpx auto;
    padding: 10rpx 2.5%;
    box-shadow: 0 0 10rpx #ccc;
    overflow: hidden;
}

.title {
    font-size: 35rpx;
}

.order-module .inline {
    font-size: 28rpx;
    padding: 10rpx 0;
    overflow: hidden;
}

.order-module .inline-zi text {
    color: #333;
}

.order-module .inline-zi {
    color: #20AD20;
    padding: 15rpx 0 10rpx;
}

.order-module .order-info {
    display: flex;
    width: 100%;
}

.order-module .order-info-item {
    flex: 1;
    text-align: center;
}

.order-module .order-info-item:first-child {
    border-right: 1rpx solid #eee;
}

.order-info-tipVal,.order-info-tip {
    margin: 10rpx 0;
}

.order-info-tipVal {
    font-size: 35rpx;
    color: #20ad20;
}

.order-info-tip {
    font-size: 30rpx;
    color: #868686;
}

.address-item {
    display: flex;
    text-align: left;
    font-size: 30rpx;
    width: 700rpx;
    margin: 20rpx auto;
}

.address-item text {
    margin-left: 20rpx;
    display: block;
}

.order-info {
    text-align: left;
    font-size: 25rpx;
    width: 600rpx;
    margin: 30rpx auto;
    overflow: hidden;
}

.text-r {
    text-align: right;
    float: right;
    display: block;
    font-size: 28rpx;
}

.order-info text:first-child,.order-info view:first-child {
    color: green;
}

.address-icon {
    width: 38rpx;
    height: 38rpx;
    position: relative;
    top: 8rpx;
}

.btn {
    background: #20AD20;
    color: #FFF;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    width: 400rpx;
    margin: 15rpx auto;
}

.soundRecordingPlay {
    height: 50rpx;
    border-radius: 50rpx;
    font-size: 20rpx;
    line-height: 50rpx;
    background: #eee;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    margin-left: 15rpx;
}

.soundRecordingPlay-left {
    display: flex;
}

.soundRecording-icon {
    width: 30rpx;
    height: 30rpx;
    padding: 10rpx;
    display: inline-block;
}

.orderItem {
    position: relative;
    width: 100%;
    height: 250rpx;
    background: #fff;
    font-size: 26rpx;
    color: #333333;
    display: flex;
    padding-top: 30rpx;
    padding-left: 25rpx;
    margin-bottom: 30rpx;
}

.orderStyle {
    width: 100%;
    height: 54rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.orderStyle .orderStyleTtile {
    font-size: 32rpx;
    width: 150rpx;
    height: 50rpx;
    align-items: center;
    overflow: hidden;
}

.orderStyle .orderStatus {
    font-size: 28rpx;
    font-weight: unset;
    color: #ff9833;
    width: 550rpx;
    text-align: right;
}

.orderStyle .orderStatus .iconfont {
    font-weight: unset;
}

.orderTime {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #333333;
    margin-top: 20rpx;
}

.orderTime image {
    display: block;
    width: 22rpx;
    height: 22rpx;
    border: 2rpx solid #a4a5a5;
    border-radius: 50%;
    margin-right: 11rpx;
}

.address {
    width: 100%;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.address-item {
    display: flex;
    text-align: left;
    font-size: 30rpx;
    width: 700rpx;
    margin: 20rpx auto;
}

.address-item text {
    margin-left: 20rpx;
    display: block;
    width: 500rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.addressProgress {
    width: 33rpx;
    height: 90rpx;
}

.icon {
    height: 40rpx;
    display: flex;
    align-items: center;
}

.start_address {
    display: block;
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background: #2e8ef0;
}

.end_address {
    display: block;
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background: #f0b72e;
}

.addressProgress view {
    height: 40rpx;
    border-left: 2rpx dotted #e7e7e7;
    margin-left: 10rpx;
}

.addressContext view:last-child {
    margin-top: 20rpx;
}

.border-top {
    margin-left: -20rpx;
    width: 140%;
    border-top: 2rpx solid #e2e2e2;
}

.orderId {
    display: flex;
    justify-content: space-between;
    color: #888;
    font-size: 25rpx;
    padding-top: 10rpx;
}

.orderStatusStyle {
    width: 0px;
    height: 0px;
    border-left: 110rpx solid transparent;
}

.orderStatus .going {
    border-top: 110rpx solid #54a8ff;
}

.orderStatus .success {
    border-top: 110rpx solid #21ab2d;
}

.orderStatus view {
    width: 110rpx;
    height: 110rpx;
    transform: rotate(45deg);
    font-size: 26rpx;
    color: #fff;
    font-weight: 600;
    position: absolute;
    right: 0rpx;
    top: 25rpx;
    z-index: 1000;
}

.font-weight {
    font-weight: bold;
    color: #333;
}

.yellow-text {
    color: #ff9833;
}

.green-text {
    color: #20ad20;
}

.red-text {
    color: #ee3112;
}

