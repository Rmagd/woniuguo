.container .None {
    display: none;
}

.container .scroll-view .None {
    display: none;
}

.container {
    padding-bottom: 100rpx;
}

.xian {
    text-align: center;
}

.xian view {
    width: 100rpx;
    height: 10rpx;
    background: #ddd;
    margin: 0 10rpx;
    display: inline-block;
    border-radius: 10rpx;
}

.xian .scrollanimation {
    background: #20AD20;
}

@keyframes headRotate {
    0% {
        background: #ddd;
    }

    100% {
        background: #20AD20;
    }

    0% {
        background: #20AD20;
    }
}

.banner {
    width: 100%;
    height: 400rpx;
}

.banner-image {
    width: 100%;
    height: 100%;
    display: block;
}

.search {
    height: 50rpx;
    display: flex;
    padding: 25rpx 20rpx;
    box-shadow: 0 0 10rpx #333;
    width: 80%;
    margin: 0 auto;
    border-radius: 10rpx;
    z-index: 99;
    position: relative;
    top: -30rpx;
    background: #fff;
}

.none {
    display: none;
}

.search-icon {
    width: 30rpx;
    height: 30rpx;
    margin: 12rpx 0 0 0;
}

.search-input {
    font-size: 28rpx;
    line-height: 50rpx;
    flex: 1;
    padding: 0 15rpx;
}

.search-btn {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 28rpx;
    background: #20ad20;
    color: #fff;
}

.sys-notify {
    height: 80rpx;
    border-bottom: 15rpx solid #f1f1f1;
}

.sys-notify-item {
    padding: 0 20rpx;
    font-size: 28rpx;
    line-height: 80rpx;
}

.sys-notify-item image {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;
    margin-top: 10rpx;
    position: relative;
    top: 5rpx;
    border-radius: 34rpx;
}

.switch-panel {
    width: 100%;
    font-size: 30rpx;
}

.switch-panel-title {
    margin: 20rpx 0;
    height: 85rpx;
    width: 100%;
    line-height: 80rpx;
    white-space: nowrap;
}

.switch-panel-title-item {
    width: 150rpx;
    text-align: center;
    display: inline-block;
    color: #333333;
}

.switch-panel-title .active {
    color: #20ad20;
    border-bottom: solid 2px #20AD20;
}

.switch-panel-content {
    margin: 20rpx 0;
}

.switch-panel-content-item {
    display: none;
    width: 100%;
}

.switch-panel-content .active {
    display: block;
}

.ware-module {
    overflow: hidden;
}

.item-img {
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
    border-radius: 10rpx;
}

.item-img image {
    display: block;
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}

.item-text {
    width: 100%;
    text-align: center;
    line-height: 40rpx;
    font-size: 28rpx;
}

.item-col-3 {
    overflow: hidden;
    margin: 10rpx 48rpx;
    width: 150rpx;
    float: left;
    border: 1rpx solid #ddd;
    padding: 20rpx 0;
    border-radius: 10rpx;
}

.item-col-4 {
    overflow: hidden;
    margin: 10rpx 17rpx;
    width: 150rpx;
    float: left;
    border: 1rpx solid #ddd;
    padding: 20rpx 0;
    border-radius: 10rpx;
}

.module-style2 {
    width: 100%;
    overflow: hidden;
}

.module-style2 .page-head {
    height: 500rpx;
    overflow: hidden;
}

.module-style2 .city-tip {
    font-size: 25rpx;
    text-align: center;
    padding: 15rpx 0;
    color: #fff;
}

.module-style2 .city-tip .icon {
    width: 30rpx;
    height: 16rpx;
    display: inline-block;
    margin-right: 15rpx;
    position: relative;
    top: -1rpx;
}

.module-style2 .user-tip {
    background: #fff;
    font-size: 26rpx;
    text-align: center;
    height: 50rpx;
    line-height: 50rpx;
    width: 350rpx;
    padding: 20rpx 0;
    margin: 50rpx auto 30rpx;
    border-radius: 50rpx;
    position: relative;
}

.module-style2 .user-tip .icon {
    width: 40rpx;
    height: 50rpx;
    position: absolute;
    left: 50rpx;
}

.module-style2 .user-swiper {
    width: 700rpx;
    height: 260rpx;
    margin: 0 auto;
    overflow: hidden;
    border-radius: 30rpx;
}

.module-style2 .scroll-view {
    width: 100%;
    height: 200rpx;
    white-space: nowrap;
    margin: 10rpx 0 0;
}

.module-style2 .scroll-view-item {
    width: 33.3%;
    height: 100%;
    display: inline-block;
    text-align: center;
}

.module-style2 .scroll-view-item image {
    width: 114rpx;
    height: 114rpx;
    margin: 40rpx auto 0;
    border-radius: 114rpx;
    display: block;
}

.module-style2 .scroll-view-item text {
    font-size: 28rpx;
}

.module-style2 .scroll-icon-group {
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
}

.module-style2 .scroll-icon-item {
    width: 15rpx;
    height: 15rpx;
    border-radius: 15rpx;
    background: #ff3a5d;
    display: inline-block;
    margin: 0 10rpx;
    transition: all 0.5s ease 0s;
}

.module-style2 .active {
    width: 30rpx;
}

.module-style2 .page-map {
    width: 100%;
    height: 90vh;
}

.module-style3 {
    width: 100%;
    overflow: hidden;
}

.module-style3 .page-map {
    width: 100%;
    height: 670rpx;
}

.module-style3 .map-upper-wrap {
    position: absolute;
    height: 120rpx;
    width: 100%;
}

.module-style3 .map-upper-btn-group {
    width: 95%;
    display: flex;
    justify-content: space-between;
    margin: 0 auto;
}

.module-style3 .map-upper-btn-item {
    width: 70rpx;
    height: 70rpx;
    margin: 20rpx 0;
}

.module-style3 .sys-notify {
    border-top: 15rpx solid #f1f1f1;
}

.module-style3 .sys-notify-icon {
    width: 34rpx;
    height: 34rpx;
    float: left;
    margin-right: 10rpx;
    position: relative;
    border-radius: 34rpx;
}

.module-style3 .sys-notify-info {
    height: 38rpx;
    line-height: 35rpx;
    font-size: 25rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    float: left;
}

.module-style3 .user-ingress {
    height: 400rpx;
    width: 100%;
    margin: 0 auto;
    overflow: hidden;
}

.module-style3 .scroll-view {
    width: 100%;
    height: 95%;
    white-space: nowrap;
    margin: 10rpx 0;
}

.module-style3 .ingress-item {
    width: 330rpx;
    height: 160rpx;
    float: left;
    background: #fff;
    margin: 15rpx 22rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.module-style3 .ingress-title {
    font-size: 30rpx;
    color: #333;
    font-weight: bold;
    margin-left: 40rpx;
    margin-top: 50rpx;
}

.module-style3 .ingress-tip {
    font-size: 25rpx;
    color: #666;
    margin-left: 45rpx;
    margin-top: 15rpx;
}

.head {
    width: 100%;
    height: 10vh;
    background: darkgoldenrod;
}

.bottom {
    color: #888;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    margin: 50rpx 0 30rpx 0;
    text-align: center;
}
.radio{
  display: block;
  padding:10rpx 0 10rpx 125rpx;
  border-bottom: solid #f1f1f1 1rpx;
}
.school{
  display: block;
  color: red;
  padding-left: 165rpx;
  margin-bottom: 30rpx;
}

.part-time{
  padding: 20rpx;
  text-align: center;
}
.jobBtn{
  width:300rpx;
  margin:30rpx auto 0;
}