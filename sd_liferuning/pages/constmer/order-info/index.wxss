page {
    background: #F2F2F2;
}

.page-head {
    background: #FFF;
    width: 100%;
    padding-bottom: 30rpx;
}

.status-view {
    font-size: 40rpx;
    line-height: 40rpx;
    margin: 20rpx 0;
    text-align: center;
}

.status-icon {
    width: 40rpx;
    height: 40rpx;
    padding: 0 15rpx;
}

.status-font {
    position: relative;
    top: -3rpx;
}

.page-content {
    background: #fff;
    width: 700rpx;
    margin: 25rpx auto;
    border-radius: 15rpx;
    padding-bottom: 30rpx;
}

.title {
    font-size: 30rpx;
    padding: 15rpx;
    font-weight: bold;
}

.data-table {
    padding: 0 15rpx;
}

.data-item {
    display: flex;
    margin: 30rpx 0 0;
    font-size: 28rpx;
    justify-content: space-between;
}

.data-item-label {
    color: #666;
    min-width: 160rpx;
}

.data-item-labe {
    color: #666;
}

.data-item-info {
    color: #222;
    text-align: left;
    overflow: hidden;
}

.data-item-btn-group {
    overflow: hidden;
    text-align: right;
}

.data-item-btn-group button {
    display: inline-block;
    font-size: 25rpx;
    margin: 0 10rpx;
    background: green;
    color: #FFF;
}

.soundRecordingPlay {
    height: 50rpx;
    width: 120rpx;
    border-radius: 50rpx;
    font-size: 20rpx;
    line-height: 50rpx;
    background: #5b9bf3;
    padding: 0 5%;
    display: flex;
    justify-content: space-between;
    text-align: right;
}

.soundRecording-icon {
    width: 22rpx;
    height: 32rpx;
    display: inline-block;
    margin: 0 10rpx 0 20rpx;
}

.image-group {
    width: 510rpx;
}

.image-group-right {
    text-align: right;
}

.image-group .image-item {
    width: 150rpx;
    height: 150rpx;
    border-radius: 15rpx;
    overflow: hidden;
    margin: 0 20rpx 15rpx 0;
}

.border-top {
    border-top: 2rpx solid #e2e2e2;
}

.title-context {
    width: 100%;
    margin: 20rpx auto;
}

.title-context .title {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.title-context .title .l {
    display: flex;
    align-items: center;
    font-size: 38rpx;
    color: #333333;
}

.title-context .title .r {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #333333;
}

.title-context .title image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.title-context .status-container {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.title-context .status-container .order-going-status {
    width: 40%;
    text-align: left;
}

.title-context .status-container .status {
    width: 60%;
    text-align: right;
}

.title-context .status view {
    display: inline-block;
    padding: 10rpx 33rpx;
    border: 1rpx solid #e2e2e2;
    margin-right: 20rpx;
    background: none;
    font-size: 26rpx;
    border-radius: 8rpx;
}

.title-context .status .s1 {
    color: #ee3112;
    border-color: #ee3112;
}

.title-context .status button {
    border: none;
    border-radius: none;
}

.again-order {
    border: 2rpx solid #ee3112;
}

.success {
    color: #20ad20;
    font-size: 28rpx;
    margin-left: 15rpx;
}

.error {
    color: #ee3112;
    font-size: 28rpx;
    margin-left: 15rpx;
}

