.iconfont {
    font-family: "iconfont"!important;
    font-size: 32rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    line-height: 64rpx;
}

.icon-right:before {
    content: "\e60f";
}

.icon-clock:before {
    content: "\e686";
}

.soundRecordingPlay-left {
    display: flex;
    align-items: center;
}

.soundRecordingPlay-left .time {
    display: inline-block;
    color: #fff;
    font-size: 26rpx;
}

.runingphone {
    display: flex;
    align-items: center;
}

.runingphone image {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 20rpx;
}

.back-to-home navigator{
  position: fixed;
  bottom: 25rpx;
  width: 700rpx;
  height: 100rpx;
  margin:0 25rpx;
  border-radius:15rpx;
  background-color: #1aad19;
  color: white;
  line-height: 100rpx;
  text-align: center;
  letter-spacing: 8rpx;
}